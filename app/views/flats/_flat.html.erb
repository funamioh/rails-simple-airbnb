<div class="card mx-3" style="width: 18rem;">
<div id="<%= dom_id flat %>">
  <img src="data:image/jpeg;base64," class="card-img-top" alt="...">
  <div class="card-body p-0">
  <div class="facility-detail-box" style="background-color: #f8f9fa;">
<div class="facility-detail p-3">
<h3><%= flat.name %></h3>
<p><%= flat.address %></p>
<div class="guests-price d-flex">
<div class="number_guests d-flex align-items-center">
<i class="fa-solid fa-user-group" style="font-size: 14px; height: 14px; line-height: 14px;"></i>
<p class="mb-0 mx-2"><%= flat.number_of_guests %></p>
</div>
<div class="price d-flex align-items-center ms-3">
<i class="fa-solid fa-yen-sign"></i>
<p class="mb-0 mx-2"><%= flat.price_per_night %></p>
</div>
</div>
</div>

<div class="links d-flex p-3" style="background-color: #ffffff;">
<p class="mb-0 text-primary"><%= link_to 'More info', flat %></p>
<p class="mb-0 ms-2 text-warning"><%= link_to 'Edit', edit_flat_path %></p>
<p class="mb-0 ms-2 text-danger">Remove</p>
</div>

</div>
</div>
</div>
</div>
